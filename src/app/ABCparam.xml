<?xml version="1.0"?>
<!DOCTYPE SEGMENTATION-PARAMETERS>
<SEGMENTATION-PARAMETERS>
<SUFFIX>EMS</SUFFIX>
<ATLAS-DIRECTORY>@ATLAS_DIR@</ATLAS-DIRECTORY>
<ATLAS-ORIENTATION>file</ATLAS-ORIENTATION>
<OUTPUT-DIRECTORY>@OUTPUT_DIR@</OUTPUT-DIRECTORY>
<OUTPUT-FORMAT>Nrrd</OUTPUT-FORMAT>
<IMAGE>
  <FILE>@T1IMG@</FILE>
  <ORIENTATION>file</ORIENTATION>
</IMAGE>
<IMAGE>
  <FILE>@T2IMG@</FILE>
  <ORIENTATION>file</ORIENTATION>
</IMAGE>
<FILTER-ITERATIONS>10</FILTER-ITERATIONS>
<FILTER-TIME-STEP>@TIME_STEP@</FILTER-TIME-STEP>
<FILTER-METHOD>Curvature flow</FILTER-METHOD>
<MAX-BIAS-DEGREE>4</MAX-BIAS-DEGREE>
<PRIOR>1.3</PRIOR>
<PRIOR>1</PRIOR>
<PRIOR>0.7</PRIOR>
<PRIOR>1</PRIOR>
<PRIOR>0.8</PRIOR>
<INITIAL-DISTRIBUTION-ESTIMATOR>robust</INITIAL-DISTRIBUTION-ESTIMATOR>
<DO-ATLAS-WARP>0</DO-ATLAS-WARP>
<ATLAS-WARP-FLUID-ITERATIONS>@ITERATIONS@</ATLAS-WARP-FLUID-ITERATIONS>
<ATLAS-WARP-FLUID-MAX-STEP>0.1</ATLAS-WARP-FLUID-MAX-STEP>
<ATLAS-LINEAR-MAP-TYPE>id</ATLAS-LINEAR-MAP-TYPE>
<IMAGE-LINEAR-MAP-TYPE>id</IMAGE-LINEAR-MAP-TYPE>
</SEGMENTATION-PARAMETERS>
